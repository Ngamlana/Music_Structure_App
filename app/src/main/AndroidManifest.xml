<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Albums"
            android:label="@string/albums_name" />
        <activity
            android:name=".Artist"
            android:label="@string/artist_name" />
        <activity
            android:name=".MusicStore"
            android:label="@string/music_store_name" />
        <activity
            android:name=".Settings"
            android:label="@string/settings_name" />
        <activity
            android:name=".NowIsPlaying"
            android:label="@string/now_is_playing_name" />
    </application>

</manifest>